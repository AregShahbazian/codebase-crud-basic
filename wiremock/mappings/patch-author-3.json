{
  "request": {
    "method": "PATCH",
    "urlPath": "/author/3",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.name == 'Sir Author 3')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Access-Control-Allow-Origin" : "*"
    },
    "bodyFileName": "author_3_updated.json",
    "transformers": [
      "response-template"
    ]
  }
}
