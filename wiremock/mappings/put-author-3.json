{
  "request": {
    "method": "PUT",
    "urlPath": "/author/3",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.name == 'Sir Author 3' && @.dateOfBirth == '03-03-1993')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "*",
      "Access-Control-Allow-Headers": "Accept, Content-Type, Content-Encoding, Server, Transfer-Encoding, X-Requested-With, X-Authorization",
      "X-Content-Type-Options": "nosniff",
      "x-frame-options": "DENY",
      "x-xss-protection": "1; mode=block",
      "Location": "/author/3"
    },
    "bodyFileName": "author_3_put.json",
    "transformers": [
      "response-template"
    ]
  }
}
